    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {}
    }
  },
  "browserslist": [
    "since 2017-06"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "babel-plugin-graphql-tag",
        {
          "strip": true
        }
      ]
    ]
